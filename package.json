{
  "name": "redblue",
  "version": "0.0.1",
  "description": "Open-Source Hypervideo Framework",
  "browser": "src/redblue-video-omni.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "[ $(git symbolic-ref --short -q HEAD) = \"npm\" ] && git checkout master -- package.json .gitignore .npmignore README.md examples && echo '\nexamples/*' >> .gitignore && rm -rf src/ && mkdir src/ && cp -r examples/modules/ src/ && sed -i '' -e 's,./examples/modules/,node_modules/redblue/src/,g' README.md",
    "watch": "bundle exec jekyll build --watch --incremental"
  },
